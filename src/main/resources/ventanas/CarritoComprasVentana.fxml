<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.alquiler.controller.CarritosCompraController">
   <children>
      <TableView fx:id="productoTable" layoutX="66.0" layoutY="67.0" prefHeight="200.0" prefWidth="502.0">
        <columns>
          <TableColumn prefWidth="166.0" text="Codigo" />
          <TableColumn prefWidth="119.0" text="Fecha" />
            <TableColumn minWidth="0.0" prefWidth="135.0" text="Sub total" />
            <TableColumn prefWidth="81.0" text="Total" />
        </columns>
      </TableView>
      <Button fx:id="regresarBtn" layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onAction="#regresarOnAction" text="Regresar" />
      <Button fx:id="eliminarCarritoBtn" layoutX="265.0" layoutY="289.0" mnemonicParsing="false" onAction="#eliminarCarritoOnAction" text="Eliminar del carrito" />
   </children>
</AnchorPane>
